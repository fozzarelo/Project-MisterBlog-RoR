<%= simple_form_for(@user) do |f| %>
<%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :email %>
    <%= f.input :password %>
 		<%= f.input :password_confirmation %>
  </div>
  <div class="form-actions">
    <%= f.submit class: 'btn btn-primary'%>
  </div>
<% end %>
